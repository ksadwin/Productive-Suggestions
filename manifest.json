{
  "manifest_version": 2,

  "name": "Be Productive",
  "description": "Gives a list of things you could do other than read Tumblr.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/glasses_favicon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}